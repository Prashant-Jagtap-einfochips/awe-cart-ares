      <TuneVar id="BasicAudioBalanceTable">
         <Name>Table</Name>
         <SubPath>Balance</SubPath>
         <Description>Define the balance curve using a table, with the first column being the list of indices and the second column being the corresponding balance gains in dB. Gain values for indices in between the elements in the table are linearly interpolated.</Description>
         <Dimensions>[30   2]</Dimensions>
         <MATLABValue>double([0 -30;10 -27;31 -14;52 -7;74 -5;95 -3;116 -1;128 0;138 -1;159 -3;180 -5;202 -7;223 -14;244 -27;255 -30])</MATLABValue>
         <DefaultValue>0 -30 10 -27 31 -14 52 -7 74 -5 95 -3 116 -1 128 0 138 -1 159 -3 180 -5 202 -7 223 -14 244 -27 255 -30</DefaultValue>
         <Tunability>3</Tunability>
         <MinValue>0</MinValue>
         <StringParse>0</StringParse>
      </TuneVar>
